@{
    ViewBag.Title = "Bootstrap Icons";
    ViewBag.subTitle = "Icons";
    Layout = null;
}


@Html.Partial("~/Views/Shared/html.cshtml")

<head>
    @Html.Partial("~/Views/Shared/title-meta.cshtml")

    @Html.Partial("~/Views/Shared/head-css.cshtml")
</head>

<body>
    <!-- Begin page -->
    <div class="wrapper">

        @Html.Partial("~/Views/Shared/menu.cshtml")

        <!-- ============================================================== -->
        <!-- Start Page Content here -->
        <!-- ============================================================== -->

        <div class="content-page">
            <div class="content">

                <!-- Start Content-->
                <div class="container-fluid">

                    @Html.Partial("~/Views/Shared/page-title.cshtml")

                    <div class="row">
                        <div class="col">
                            <div class="card">
                                <div class="card-body">
                                    <h5 class="card-title">Icons</h5>
                                    <p class="text-muted">
                                        Use class
                                        <code>&lt;i class=&quot;bi bi-123&quot;&gt;&lt;/i&gt;</code>
                                    </p>
                                    <div class="row icons-list-demo" id="bootstrap-icons"></div>
                                </div> <!-- end card body -->
                            </div> <!-- end card -->
                        </div> <!-- end col -->
                    </div> <!-- end row -->

                </div> <!-- container -->

            </div> <!-- content -->
            @Html.Partial("~/Views/Shared/footer.cshtml")

        </div>

        <!-- ============================================================== -->
        <!-- End Page content -->
        <!-- ============================================================== -->

    </div>
    <!-- END wrapper -->
    @Html.Partial("~/Views/Shared/right-sidebar.cshtml")

    @Html.Partial("~/Views/Shared/footer-scripts.cshtml")

    <!-- Remixicons Icons Demo js -->
    <script src="~/assets/js/pages/icons-bootstrap.init.js"></script>

    <!-- App js -->
    <script src="~/assets/js/app.min.js"></script>

</body>

</html>